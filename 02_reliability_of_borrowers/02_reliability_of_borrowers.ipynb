   "source": [
    "#Лемматизация столбца purpose\n",
    "from pymystem3 import Mystem\n",
    "m = Mystem()\n",
    "def purpose_lemma(row):\n",
    "    return m.lemmatize(row['purpose'])\n",
    "df['purpose_lemma'] = df.apply(purpose_lemma, axis = 1)\n",
    "\n",
    "#Создание обобщенных групп для целей получения кредита\n",
    "def purpose_group(row):\n",
    "    words = row['purpose_lemma']\n",
    "    if 'автомобиль' in words:\n",
    "        return 'автомобиль'\n",
    "    elif 'свадьба' in words:\n",
    "        return  'свадьба'\n",
    "    elif 'жилье' in words or 'недвижимость' in words:\n",
    "        return 'недвижимость'\n",
    "    elif 'образование' in words:\n",
    "        return 'образование'\n",
    "    else:\n",
    "        return row['purpose_lemma']\n",
    "df['purpose_group'] = df.apply(purpose_group, axis = 1) \n",
    "\n",
    "\n",
    "df.head(20)\n",
    "\n",
    "df['purpose_group'].value_counts()"
